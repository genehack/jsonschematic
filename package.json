{
   "bin": {
      "jsonschematic": "bin/jsonschematic.js"
   },
   "bugs": "https://github.com/yanick/jsonschematic/issues",
   "contributors": [
      {
         "name": "Yanick Champaux",
         "email": "yanick.champoux@iinteractive.com"
      },
      {
         "name": "Trey Bianchini",
         "email": "trey.bianchini@iinteractive.com"
      }
   ],
   "dependencies": {
      "compression": "1.7.4",
      "css-loader": "^3.6.0",
      "globby": "11.0.1",
      "jsonpointer": "4.1.0",
      "lodash": "4.17.15",
      "polka": "next",
      "sirv": "1.0.1",
      "style-loader": "^1.2.1",
      "yargs": "^15.3.1"
   },
   "description": "json schema viewer",
   "devDependencies": {
      "@apidevtools/json-schema-ref-parser": "9.0.1",
      "@babel/core": "7.10.4",
      "@babel/plugin-syntax-dynamic-import": "7.8.3",
      "@babel/plugin-transform-runtime": "7.10.4",
      "@babel/preset-env": "7.10.4",
      "@babel/runtime": "7.10.4",
      "@storybook/addon-actions": "5.3.19",
      "@storybook/addon-knobs": "5.3.19",
      "@storybook/addon-notes": "5.3.19",
      "@storybook/svelte": "5.3.19",
      "@testing-library/svelte": "3.0.0",
      "babel-loader": "8.1.0",
      "eslint": "7.4.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-babel": "5.3.1",
      "eslint-plugin-prettier": "3.1.4",
      "eslint-plugin-svelte3": "2.7.3",
      "jsdom": "^16.2.2",
      "jsdom-global": "^3.0.2",
      "lint-diff": "1.2.1",
      "mini-css-extract-plugin": "^0.9.0",
      "npm-run-all": "4.1.5",
      "prettier": "2.0.5",
      "prettier-plugin-svelte": "1.1.0",
      "require-extension-hooks": "^0.3.3",
      "require-extension-hooks-module": "^0.1.4",
      "sapper": "0.27.16",
      "standard-version": "8.0.0",
      "svelte": "3.23.2",
      "svelte-loader": "2.13.6",
      "svelte-test": "^0.4.0",
      "sveltejs-tippy": "3.0.0",
      "tap": "^14.10.7",
      "webpack": "^4.43.0",
      "webpack-cli": "^3.3.12"
   },
   "eslintConfig": {
      "extends": [
         "prettier"
      ],
      "ignorePatterns": [
         "src/node_modules"
      ],
      "overrides": [
         {
            "files": [
               "*.svelte"
            ],
            "processor": "svelte3/svelte3"
         }
      ],
      "parserOptions": {
         "ecmaFeatures": {
            "modules": true
         },
         "ecmaVersion": "2020",
         "sourceType": "module"
      },
      "plugins": [
         "svelte3"
      ],
      "rules": {}
   },
   "homepage": "https://yanick.github.io/jsonschematic/",
   "keywords": [
      "json schema"
   ],
   "license": "MIT",
   "main": "__sapper__/build/index.js",
   "name": "@infinity-interactive/jsonschematic",
   "prettier": {
      "svelteSortOrder": "markup-scripts-styles",
      "svelteStrictMode": false
   },
   "repository": "github:yanick/jsonschematic",
   "scripts": {
      "build": "sapper build",
      "build:docs": "webpack -c ./webpack.docs.config.js",
      "cy:open": "cypress open",
      "cy:run": "cypress run",
      "dev": "sapper dev",
      "export": "sapper export --legacy",
      "lint": "npm-run-all --parallel \"lint:prettier -- {@}\" \"lint:eslint -- {@}\" --",
      "lint:eslint": "./scripts/eslint",
      "lint:eslint:fix": "npm run lint:eslint -- --fix",
      "lint:fix": "npm-run-all \"lint:eslint:fix -- {@}\" \"lint:prettier:fix -- {@}\" --",
      "lint:prettier": "./scripts/prettier",
      "lint:prettier:fix": "npm run lint:prettier -- --fix",
      "package": "./scripts/merge_package.pl",
      "promake": "./Promake",
      "release": "standard-version",
      "start": "node __sapper__/build",
      "storybook": "start-storybook",
      "test": "tap 'src/**/*test.js' --no-coverage"
   },
   "version": "0.0.1"
}
